// Place your key bindings in this file to override the defaults
[
    {
        "key": "cmd+k h",
        "command": "workbench.action.showEditorsInActiveGroup"
    },
    {
        "key": "ctrl+alt+s",
        "command": "workbench.action.files.saveWithoutFormatting"
    },
    {
        "key": "cmd+k s",
        "command": "-workbench.action.files.saveWithoutFormatting"
    },
    {
        "key": "cmd+k s",
        "command": "outline.focus"
    }
]